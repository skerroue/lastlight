<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane maxHeight="850.0" maxWidth="800.0" minHeight="850.0" minWidth="800.0" prefHeight="850.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controler.Controler">
   <children>
      <Pane fx:id="tileContainer" layoutY="50.0" maxHeight="800.0" maxWidth="800.0" minHeight="800.0" minWidth="800.0" prefHeight="800.0" prefWidth="800.0" />
      <Pane fx:id="entityContainer" focusTraversable="true" layoutY="50.0" maxHeight="800.0" maxWidth="800.0" minHeight="800.0" minWidth="800.0" onKeyPressed="#onKeyPressed" onKeyReleased="#onKeyReleased" prefHeight="800.0" prefWidth="800.0" />
      <Pane fx:id="interfaceContainer" maxHeight="850.0" maxWidth="800.0" minHeight="850.0" minWidth="800.0" prefHeight="850.0" prefWidth="800.0">
         <children>
            <Rectangle height="50.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="800.0" />
         </children></Pane>
      <Pane fx:id="pausePane" maxHeight="850.0" maxWidth="800.0" minHeight="850.0" minWidth="800.0" prefHeight="850.0" prefWidth="800.0" visible="false">
         <children>
            <ImageView fitHeight="850.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../img/pause-background.png" />
               </image>
            </ImageView>
            <Button fx:id="resumeButton" layoutX="278.0" layoutY="283.0" mnemonicParsing="false" onAction="#resume" style="-fx-background-color: transparent;">
               <graphic>
                  <ImageView fitHeight="92.0" fitWidth="259.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../img/resume-button.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="quitButton" layoutX="300.0" layoutY="374.0" mnemonicParsing="false" onAction="#quit" style="-fx-background-color: transparent;">
               <graphic>
                  <ImageView fitHeight="95.0" fitWidth="212.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../img/quit-button.png" />
                     </image>
                  </ImageView>
               </graphic></Button>
         </children></Pane>
   </children>
</Pane>
